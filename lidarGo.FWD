#!/bin/sh
##########################################
# lidarGo.FWD - Pretend LIDAR always says "GO FORWARD!"
##########################################

# Constants that could be returned upon exit
EXIT_DIR_UNKNOWN=0
EXIT_DIR_ERROR=1
EXIT_DIR_LEFT=2
EXIT_DIR_FWD=3
EXIT_DIR_RIGHT=4
EXIT_DIR_BACKUP_AND_TURN=100
EXIT_DIR_STUCK=124

# Default to GOING FORWARD!
DIRECTION=$EXIT_DIR_FWD

exit $DIRECTION

